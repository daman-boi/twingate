{
  "redirects": [
    {
      "source": "https://:host/:path*",
      "destination": "https://:host/:path*",
      "permanent": true
    },
    {
      "source": "/:path*",
      "has": [
        {
          "type": "host",
          "value": "*.twingate.me"
        }
      ],
      "destination": "https://twingate.me/:path*",
      "permanent": true
    }
  ]
}
